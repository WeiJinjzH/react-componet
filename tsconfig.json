{
    "compilerOptions": {
        "baseUrl": ".",
        "outDir": "./dist/",
        "allowJs": true,
        "sourceMap": true,
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es5",
        "jsx": "react",
        "downlevelIteration": true,
        "paths": {
            "src": ["./src"],
            "utils": ["./src/utils"]
        }
    },
    "exclude": [
        "node_modules"
    ]
}