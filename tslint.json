{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "semi": [true, "never"],
        "ter-indent":[true, 4],
        "max-line-length": [true, 140],
        "no-boolean-literal-compare": false,
        "jsx-no-multiline-js": false,
        "jsx-no-lambda": false,
        "import-name": false,
        "no-implicit-dependencies": [true, "optional"]
    },
    "rulesDirectory": []
}